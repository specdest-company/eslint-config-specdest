{
  "name": "@specdest-company/eslint-config-specdest",
  "version": "0.0.3",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "files": [
    "./base.js",
    "./react.js"
  ],
  "scripts": {
    "fmt": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "fmt:fix": "pnpm fmt -w",
    "clean": "rm -rf .turbo node_modules pnpm-lock.yaml",
    "release": "pnpm run version && git push --follow-tags",
    "commit": "cz",
    "version": "standard-version",
    "prepare": "husky"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "14.0.4",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "@typescript-eslint/eslint-plugin-tslint": "6.16.0",
    "@typescript-eslint/parser": "6.16.0",
    "babel-eslint": "10.1.0",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-config-turbo": "1.11.2",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-boundaries": "4.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-testing-library": "6.2.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tslint": "6.1.3"
  },
  "devDependencies": {
    "@specdest-company/tool_config_prettier": "github:specdest-company/tool_config_prettier#v0.0.3",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "commitizen": "4.3.0",
    "conventional-changelog-cli": "4.1.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "9.0.11",
    "prettier": "3.2.5",
    "standard-version": "9.5.0",
    "typescript": "5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "./base.js"
    ]
  },
  "prettier": "@specdest-company/tool_config_prettier",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": ">= 8"
  }
}
